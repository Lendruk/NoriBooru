{
	"name": "@nori-booru/backend",
	"version": "0.5.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"generate:drizzle:master": "drizzle-kit generate --config=./master.drizzle.config.ts",
		"generate:drizzle:vault": "drizzle-kit generate --config=./vault.drizzle.config.ts",
		"generate:drizzle": "npm run generate:drizzle:master && npm run generate:drizzle:vault",
		"drizzle:drop:migration": "drizzle-kit drop --config=./vault.drizzle.config.ts",
		"build": "rm -rf dist && node ./config.esbuild.mjs",
		"build:tsc": "rm -rf dist && tsc",
		"dev": "nodemon ./src/main.ts --config nodemon.json",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"bump:patch": "npm version patch --git-tag-version false",
		"bump:minor": "npm version minor --git-tag-version false",
		"bump:major": "npm version major --git-tag-version false"
	},
	"keywords": [],
	"author": "Lendruk",
	"license": "MIT",
	"dependencies": {
		"@fastify/cors": "^9.0.1",
		"@fastify/multipart": "^8.1.0",
		"@fastify/websocket": "^10.0.1",
		"tree-kill": "^1.2.2",
		"fastify": "^4.27.0",
		"drizzle-orm": "^0.31.2",
		"exifreader": "^4.23.1",
		"better-sqlite3": "^9.4.0",
		"fluent-ffmpeg": "^2.1.2",
		"sharp": "^0.33.2"
	},
	"bundleDependencies": [
		"@fastify/multipart",
		"@fastify/cors",
		"@fastify/websocket",
		"fastify",
		"drizzle-orm",
		"exifreader",
		"tree-kill"
	],
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.9",
		"@types/fluent-ffmpeg": "^2.1.24",
		"@types/node": "^20.11.16",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^7.3.1",
		"drizzle-kit": "^0.22.7",
		"esbuild": "^0.23.0",
		"eslint": "^8.56.0",
		"nodemon": "^3.0.3",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
